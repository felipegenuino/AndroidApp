<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.felipegenuino.felipegenuino" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Sobre"
            android:label="@string/title_activity_sobre"
            android:parentActivityName=".Principal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.felipegenuino.felipegenuino.Principal" />
        </activity>
        <activity
            android:name=".Trabalhos"
            android:label="@string/title_activity_trabalhos"
            android:parentActivityName=".Principal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.felipegenuino.felipegenuino.Principal" />
        </activity>
        <activity
            android:name=".Contato"
            android:label="@string/title_activity_contato"
            android:parentActivityName=".Principal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.felipegenuino.felipegenuino.Principal" />
        </activity>
        <activity
            android:name=".Acesso"
            android:label="@string/title_activity_acesso" >
        </activity>
    </application>

</manifest>
